config_version=5

[application]

config/name="Pocket Office"
config/description="A Kairosoft-inspired corporate simulation game"
config/version="0.1.0"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
